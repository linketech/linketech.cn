{
	"name": "linketech-website",
	"version": 2,
	"builds": [
		{ "src": "/official-website/build/**/*", "use": "@now/static" },
		{ "src": "/admin/build/**/*", "use": "@now/static" },
		{ "src": "/index.js", "use": "@now/node-server" }
	],
	"routes": [
		{ "src": "/home", "dest": "/official-website" },
		{ "src": "/core-tech", "dest": "/official-website" },
		{ "src": "/smart-parking", "dest": "/official-website" },
		{ "src": "/accurate-locating", "dest": "/official-website" },
		{ "src": "/wireless-router", "dest": "/official-website" },
		{ "src": "/about/(.*)", "dest": "/official-website" },
		{ "src": "/admin", "dest": "/admin" },
		{ "src": "/admin/static/(.*)", "dest": "/admin/static/$1" },
		{ "src": "/api(.*)", "dest": "/" },
		{ "src": "/static/(.*)", "dest": "/official-website/static/$1" },
		{ "src": "/(.*)", "dest": "/official-website/$1" }
	],
	"env": {
		"JWT_SECRET": "@jwt-secret",
		"MONGODB_DB": "@mongodb-db",
		"MONGODB_USER": "@mongodb-user",
		"MONGODB_PASSWORD": "@mongodb-password"
	}
}
